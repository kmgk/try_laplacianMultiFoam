FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue DT 4e-7
);

regions
(
    boxToCell
    {
        box (0 0 125e-6) (100e-6 100e-6 175e-6);
        fieldValues
        (
            volScalarFieldValue DT 6e-8
        );
    }
);


